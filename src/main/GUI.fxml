        <HBox alignment="CENTER" translateX="100.0" translateY="-5.0" GridPane.columnIndex="4" GridPane.rowIndex="1">
            <Circle fx:id="sonar5Led" radius="4.0" fill="darkgray" />
            <Label fx:id="lblSensor5" text=" 0.0m" />
        </HBox>

