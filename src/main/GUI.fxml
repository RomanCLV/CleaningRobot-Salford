                <graphic>
                    <ImageView fitHeight="75.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/arrowUp.png"/>
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="btnLeft" maxHeight="75.0" maxWidth="75.0"
